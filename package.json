{
  "name": "martylouis-com",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "description": "Personal website of Marty Thierry, built with Astro.",
  "license": "MIT",
  "author": "Marty Thierry <marty@martylouis.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/martylouis/martylouis.com.git"
  },
  "bugs": {
    "url": "https://github.com/martylouis/martylouis.com/issues"
  },
  "homepage": "https://github.com/martylouis/martylouis.com#readme",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro check && astro build && astro preview",
    "astro": "astro",
    "lint": "npm-run-all --continue-on-error lint:*",
    "lint:astro": "astro check",
    "lint:imports": "organize-imports-cli ./tsconfig.json",
    "lint:eslint": "eslint . --fix --cache --ignore-path .gitignore",
    "lint:prettier": "prettier --write \"**/*.{ts,js,jsx,tsx,md,mdx,astro}\" --cache --list-different --ignore-path .gitignore --plugin=prettier-plugin-astro",
    "toolbar:disable": "astro preferences disable devToolbar",
    "toolbar:enable": "astro preferences enable devToolbar",
    "ui:add": "pnpm dlx shadcn-ui add",
    "ui:diff": "pnpm dlx shadcn-ui diff"
  },
  "dependencies": {
    "@astrojs/mdx": "^4.0.2",
    "@astrojs/react": "^4.1.1",
    "@astrojs/tailwind": "^5.1.3",
    "@iconify-json/radix-icons": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@tailwindcss/vite": "4.0.0-alpha.3",
    "astro": "^5.0.9",
    "astro-icon": "^1.1.0",
    "astro-m2dx": "^0.7.16",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "framer-motion": "^10.18.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sharp": "^0.33.4",
    "tailwind-merge": "^2.4.0",
    "tailwindcss": "4.0.0-alpha.3",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/partytown": "^2.1.2",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.13",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "astro-eslint-parser": "^0.16.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-astro": "^0.30.0",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-tailwindcss": "^3.17.4",
    "npm-run-all": "^4.1.5",
    "organize-imports-cli": "^0.10.0",
    "prettier": "^3.3.3",
    "prettier-plugin-astro": "^0.12.3",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "typescript": "^5.5.4"
  },
  "packageManager": "pnpm@8.12.1"
}
