{
  "name": "martylouis-com",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "description": "Personal website of Marty Thierry, built with Astro.",
  "license": "MIT",
  "author": "Marty Thierry <marty@martylouis.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/martylouis/martylouis.com.git"
  },
  "bugs": {
    "url": "https://github.com/martylouis/martylouis.com/issues"
  },
  "homepage": "https://github.com/martylouis/martylouis.com#readme",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro check && astro build && astro preview",
    "astro": "astro",
    "lint": "npm-run-all --continue-on-error lint:*",
    "lint:astro": "astro check",
    "lint:imports": "organize-imports-cli ./tsconfig.json",
    "lint:eslint": "eslint . --fix --cache --ignore-path .gitignore",
    "lint:prettier": "prettier --write \"**/*.{ts,js,jsx,tsx,md,mdx,astro}\" --cache --list-different --ignore-path .gitignore",
    "toolbar:disable": "astro preferences disable devToolbar",
    "toolbar:enable": "astro preferences enable devToolbar"
  },
  "dependencies": {
    "@astrojs/mdx": "^2.0.0",
    "@astrojs/react": "^3.0.7",
    "@astrojs/tailwind": "^5.0.3",
    "@radix-ui/react-icons": "^1.3.0",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "astro": "^4.0.3",
    "astro-icon": "^0.8.2",
    "astro-m2dx": "^0.7.16",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "tailwind-merge": "^2.1.0",
    "tailwindcss": "^3.3.6",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@astrojs/check": "^0.3.1",
    "@astrojs/partytown": "^2.0.2",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "astro-eslint-parser": "^0.16.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-astro": "^0.30.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "npm-run-all": "^4.1.5",
    "organize-imports-cli": "^0.10.0",
    "prettier": "^3.1.0",
    "prettier-plugin-astro": "^0.12.2",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "typescript": "^5.3.3"
  },
  "packageManager": "pnpm@8.11.0"
}
