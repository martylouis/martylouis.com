{
  "name": "martylouis-gatsby",
  "version": "1.0.0",
  "description": "Terms of Service for MartyLouisCo",
  "main": "index.js",
  "scripts": {
    "analyze:lint": "eslint --ext .jsx --ext .js .",
    "analyze:prettier": "prettier --list-different '**/*.{js,css,md,json}'",
    "analyze": "npm run analyze:lint && npm run analyze:prettier",
    "build": "gatsby build",
    "dev": "gatsby develop",
    "fix:lint": "eslint --ext .jsx --ext .js . --fix",
    "fix:prettier": "prettier --write '**/*.{js,css,md,json}'",
    "fix": "npm run fix:lint && npm run fix:prettier",
    "start": "npm run dev",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/martylouis/martylouis-gatsby.git"
  },
  "keywords": [],
  "author": "Marty Thierry <marty@martylouis.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/martylouis/martylouis-gatsby/issues"
  },
  "homepage": "https://github.com/martylouis/martylouis-gatsby#readme",
  "private": true,
  "dependencies": {
    "@mdx-js/mdx": "^1.5.7",
    "@mdx-js/react": "^1.5.7",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "gatsby": "^2.20.2",
    "gatsby-plugin-manifest": "^2.3.1",
    "gatsby-plugin-mdx": "^1.1.0",
    "gatsby-plugin-offline": "^3.1.0",
    "gatsby-plugin-postcss": "^2.2.0",
    "gatsby-plugin-purgecss": "^4.0.1",
    "gatsby-plugin-react-helmet": "^3.2.0",
    "gatsby-plugin-web-font-loader": "^1.0.4",
    "gatsby-remark-prismjs": "^3.4.0",
    "gatsby-remark-vscode": "^2.0.3",
    "gatsby-source-filesystem": "^2.2.1",
    "gatsby-transformer-remark": "^2.7.0",
    "graphql": "^14.6.0",
    "material-palenight-theme": "github:whizkydee/vscode-material-palenight-theme",
    "prismjs": "^1.19.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^5.2.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
